{
    "name": "cometkim-blog-src",
    "description": "Source of Hyeseong's Blog",
    "version": "0.0.1",
    "author": "Hyeseong Kim <cometkim.kr@gmail.com>",
    "keywords": [
        "gatsby",
        "typescript",
        "blog"
    ],
    "license": "MIT",
    "main": "n/a",
    "private": true,
    "scripts": {
        "update-posts": "git submodule update --init --remote",
        "build": "yarn update-posts && gatsby build",
        "develop": "gatsby develop",
        "format": "tslint --fix -c tslint.json src/**/*.tsx?",
        "test": "jest"
    },
    "jest": {
        "verbose": true,
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/.cache/"
        ],
        "modulePaths": [
            "<rootDir>/src/"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx",
            "json"
        ]
    },
    "dependencies": {
        "gatsby": "^2.3.4",
        "gatsby-plugin-canonical-urls": "2.0.12",
        "gatsby-plugin-feed": "2.1.0",
        "gatsby-plugin-google-analytics": "2.0.18",
        "gatsby-plugin-manifest": "2.0.27",
        "gatsby-plugin-netlify": "2.0.13",
        "gatsby-plugin-offline": "2.0.25",
        "gatsby-plugin-react-helmet": "^3.0.11",
        "gatsby-plugin-resolve-src": "2.0.0",
        "gatsby-plugin-sharp": "2.0.32",
        "gatsby-plugin-sitemap": "2.0.11",
        "gatsby-plugin-styled-components": "3.0.7",
        "gatsby-plugin-twitter": "2.0.13",
        "gatsby-plugin-typescript": "^2.0.11",
        "gatsby-remark-autolink-headers": "2.0.16",
        "gatsby-remark-copy-linked-files": "2.0.11",
        "gatsby-remark-embed-youtube": "0.0.7",
        "gatsby-remark-emojis": "0.3.2",
        "gatsby-remark-external-links": "0.0.4",
        "gatsby-remark-images": "3.0.10",
        "gatsby-remark-katex": "3.0.4",
        "gatsby-remark-prismjs": "3.2.7",
        "gatsby-remark-responsive-iframe": "2.1.1",
        "gatsby-source-filesystem": "2.0.28",
        "gatsby-transformer-remark": "2.3.8",
        "katex": "0.10.1",
        "prismjs": "1.16.0",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "react-helmet": "5.2.0",
        "react-icons-kit": "1.2.1",
        "remark-parse": "6.0.3",
        "styled-components": "4.1.3"
    },
    "devDependencies": {
        "@babel/core": "7.4.3",
        "@types/jest": "24.0.11",
        "@types/node": "11.12.3",
        "@types/react": "16.8.13",
        "@types/react-dom": "16.8.3",
        "@types/react-helmet": "5.0.8",
        "babel-core": "7.0.0-bridge.0",
        "babel-plugin-styled-components": "1.10.0",
        "jest": "24.3.1",
        "tslint": "5.13.1",
        "typescript": "^3.2.2"
    }
}
